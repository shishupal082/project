ADJUSTABLE KEY.ON.DELAY:		0;			// KEY.ON.DELAY AND KEY.OFF.DELAY:	0-280	FOR
ADJUSTABLE KEY.OFF.DELAY:		12;			// GENISYS AND 8-280 FOR MICROLOK PROTOCOL
ADJUSTABLE GRANT.DELAY:			1000:MSEC;		// GENISYS AND 8-280 FOR MICROLOK PROTOCOL
ADJUSTABLE POINT.POINT:			1;			// 0-FOR SLAVE PORT, 1-FOR MASTER PORT
